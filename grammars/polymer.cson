      '1':
        'name': 'punctuation.definition.tag.begin.polymer'
      '2':
        'name': 'entity.name.tag.other.polymer'
    'end': '(>)'
    'endCaptures':
      '1':
        'name': 'punctuation.definition.tag.end.polymer'
    'name': 'meta.tag.other.polymer'
    'patterns': [
      {
        'include': '#tag-stuff'
      }
    ]
  }
  {
    'include': '#entities'
  }
  {
    'match': '<>'
